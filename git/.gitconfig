[commit]
	gpgsign = true
[core]
	excludesfile = ~/.gitignore_global
	editor = /usr/local/bin/vim
[diff]
	compactionHeuristic = true
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	required = true
[gpg]
	program = gpg2
[interactive]
	diffFilter = diff-highlight
