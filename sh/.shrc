## Exports
#
export EDITOR=vim                   # default editor
export GETPATH=$HOME/src            # git-get
export GO111MODULE=auto             # go mod
export GOPATH=$HOME/go              # go code home
export GPG_TTY=$(tty)               # gpg
export HOMEBREW_EDITOR=$EDITOR      # brew edit
export PATH=$GOPATH/bin:$PATH       # go bin
export PATH=$HOME/.cargo/bin:$PATH  # rust bin
export PATH=$HOME/.krew/bin:$PATH   # krew bin

## Initializations
#
if [ -e /usr/local/Caskroom/google-cloud-sdk/latest/google-cloud-sdk/path.zsh.inc ]; then . /usr/local/Caskroom/google-cloud-sdk/latest/google-cloud-sdk/path.zsh.inc; fi
if [ -e $HOME/.nix-profile/etc/profile.d/nix.sh ]; then . $HOME/.nix-profile/etc/profile.d/nix.sh; fi # added by Nix installer

## Aliases
#
alias k="kubectl"
